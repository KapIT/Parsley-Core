<mx:Object 
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:com="org.spicefactory.parsley.tag.command.*"
	xmlns:link="org.spicefactory.parsley.tag.command.link.*"
	xmlns:lc="org.spicefactory.parsley.tag.lifecycle.*"
	xmlns:flextag="org.spicefactory.parsley.tag.core.*">
	
	<mx:Script>
		<![CDATA[

			import org.spicefactory.parsley.command.target.AsyncCommand;
			import org.spicefactory.parsley.command.target.AsyncCommandWithWrappedResult;

		]]>
	</mx:Script>
	
	
	<com:CommandFactoryTag>
		<com:CommandFlowTag>
			<com:CommandTag type="{AsyncCommandWithWrappedResult}">
				<com:config>
					<flextag:PropertyTag name="result" value="1"/>
				</com:config>
				<link:LinkResultPropertyTag name="property" value="1" to="{second}"/>
			</com:CommandTag>
			<com:CommandTag type="{AsyncCommand}" id="second">
				<com:config>
					<flextag:PropertyTag name="result" value="2"/>
				</com:config>
			</com:CommandTag>
		</com:CommandFlowTag>
	</com:CommandFactoryTag>


</mx:Object>